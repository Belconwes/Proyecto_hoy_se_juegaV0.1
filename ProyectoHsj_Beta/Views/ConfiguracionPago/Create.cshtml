@model ConfiguracionPago

@{

    ViewData["Title"] = "Agregar Nueva Configuración de Pago";
}

<h2>Agregar Nueva Configuración de Pago</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="MontoSena" class="control-label">Monto Seña</label>
        <input asp-for="MontoSena" class="form-control" />
        <span asp-validation-for="MontoSena" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </div>
</form>
